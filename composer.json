{
  "name": "pmarien/rest-api-response",
  "description": "This library provides a extension for the symfony/http-foundation component to build standardized json responses.",
  "type": "library",
  "require": {
    "php": ">=5.4",
    "symfony/http-foundation": "^2.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0",
    "satooshi/php-coveralls": "^0.6.1"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Philipp Marien",
      "email": "philipp.marien@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "PMarien\\RestApiResponse\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PMarien\\RestApiResponse\\Tests\\": "tests"
    }
  },
  "minimum-stability": "stable"
}
